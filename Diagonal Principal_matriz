

#include "stdafx.h"
#include <iostream> 
#include <conio.h> 

using namespace std;

void main(){ 
int m[3][3],dp; 

for(int i=1;i<=3;i++){ 
for(int j=1;j<=3;j++){ 
cout<<"ingrese elementos para la matriz ["<<i<<"] ["<<j<<"]: "; 
cin>>m[i][j]; 
} 
} 

for(int i=1;i<=3;i++){ 
cout<<"\n "; 
for(int j=1;j<=3;j++){ 
cout<<m[i][j]; 
} 
} 
cout<<"diagaonal principal es: "; 

for(int i=1;i<=3;i++){ 
for(int j=1;j<=3;j++){ 
if(i==j){ 
dp=m[i][j]; 
cout<<"\n "<<dp; 
} 

} 
} 
getch(); 
} 
